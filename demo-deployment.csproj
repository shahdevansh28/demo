    <ImplicitUsings>enable</ImplicitUsings>
    <RootNamespace>demo_deployment</RootNamespace>
    <UserSecretsId>28b30d28-bb98-4631-bca8-c8b7cb9d012c</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
